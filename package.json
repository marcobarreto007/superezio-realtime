{
  "name": "superezio-realtime",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "overrides": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently \"npm run serve:watch\" \"npm run dev\"",
    "dev:all": "concurrently \"npm run python:serve\" \"npm run serve:watch\" \"npm run dev\"",
    "build": "npx tsc && vite build",
    "preview": "vite preview",
    "serve": "tsx server.ts",
    "serve:watch": "tsx watch server.ts",
    "start": "npm run serve",
    "python:serve": "cd backend && venv\\Scripts\\python.exe api.py",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:python": "cd backend && venv\\Scripts\\python.exe test_config.py",
    "test:all": "npm run test:run && npm run test:python"
  },
  "dependencies": {
    "@types/express-fileupload": "^1.5.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "compression": "^1.7.4",
    "express": "^4.21.1",
    "express-fileupload": "^1.5.2",
    "fs-extra": "^11.3.2",
    "googleapis": "^166.0.0",
    "highlight.js": "^11.11.1",
    "http-proxy-middleware": "^3.0.3",
    "imap": "^0.8.19",
    "lucide-react": "^0.554.0",
    "mailparser": "^3.9.0",
    "marked": "^17.0.0",
    "nodemailer": "^7.0.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/imap": "^0.8.42",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/ui": "^4.0.8",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "happy-dom": "^20.0.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "ts-prune": "^0.10.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vitest": "^4.0.8"
  }
}
